	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0X10001000
	DCD Reset_Handler
	ALIGN
	AREA mycode,CODE,READONLY
	ENTRY
	EXPORT Reset_Handler
Reset_Handler
	LDR R0,=V1
	LDR R1,=ANS
	LDR R2,[R0]
	MLA R3,R2,R2,R2
	LSR R3,#1
	STR R3,[R1]
STOP
	B STOP
V1 DCD 5
	AREA mydata,DATA,READWRITE
ANS DCD 0
	END
