	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0X10001000
	DCD Reset_Handler
	ALIGN
	AREA mycode,CODE,READONLY
	ENTRY
	EXPORT Reset_Handler
Reset_Handler
	LDR R0,=SRC
	MOV R4,#10
	LDR R1,=DST
UP	LDR R2,[R0],#4
	ADDS R3,R3,R2
	ADC R5,#0
	SUBS R4,#1
	BNE UP
	STR R3,[R1],#4
	STR R5,[R1]
STOP B STOP
SRC DCD 0X12345678,0X23232323,0XABCDEF12,0X12,0X23456789,0X10,0XABCDE,0X11111111,0X12345123,0X15
	AREA mydata,DATA,READWRITE
DST DCD 0,0
	END
	
