	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0X10001000
	DCD Reset_Handler
	ALIGN
	AREA mycode,CODE,READONLY
	ENTRY
	EXPORT Reset_Handler
Reset_Handler
	LDR R0,=DST
	LDR R1,=0X1234ABCD
	MOV R2,#8
UP	AND R3,R1,#0X0F
	CMP R3,#0X0A
	BCC down
	ADD R3,#0X07
down ADD R3,#0X30
	STRB R3,[R0],#1
	ROR R1,#4
	SUBS R2,#1
	BNE UP
STOP
	B STOP
	AREA mydata,DATA,READWRITE
DST DCD 0X10001000
	END
